{
  "description": "Laravel Redis ORM",
  "name": "dekmabot/laravel-redis-orm",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "ext-redis": "*",
    "laravel/framework": "^8.0|^9.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.13",
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "LaravelRedisOrm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
